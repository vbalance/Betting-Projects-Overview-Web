{
  "name": "betting-projects-overview",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.47",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "deploy:aws": "./scripts/deploy.sh",
    "aws:create-stack": "aws cloudformation create-stack --stack-name betting-overview-infrastructure --template-body file://aws/cloudformation.yml --region us-east-1",
    "aws:update-stack": "aws cloudformation update-stack --stack-name betting-overview-infrastructure --template-body file://aws/cloudformation.yml --region us-east-1",
    "aws:delete-stack": "aws cloudformation delete-stack --stack-name betting-overview-infrastructure --region us-east-1",
    "aws:stack-status": "aws cloudformation describe-stacks --stack-name betting-overview-infrastructure --region us-east-1 --query 'Stacks[0].{Status:StackStatus,Outputs:Outputs}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
